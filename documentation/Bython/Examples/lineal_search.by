def linear_search(arr, target) {
    for i in range(len(arr)) {
        if (arr[i] == target) {
            return i;
        }
    }
    return -1;
}

arr = [1, 3, 5, 7, 9];
target = 7;
index = linear_search(arr, target);
if (index != -1) {
    print("Elemento encontrado en el Ã­ndice");
} else {
    print("Elemento no encontrado");
}
